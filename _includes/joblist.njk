<div class="grid grid-cols-1 md:grid-cols-2 gap-8">
  {% for job in joblist %}
    <div>
      <h2>
        <span class="font-mono font-normal">{{ job.data.date | date }}</span>
        <span>{{ job.data.title }}</span>
        <a class="text-gray-300 dark:text-gray-600" href="{{ job.data.website }}" target="_blank" rel="noopener noreferrer">#</a>
      </h2>
      <span>{{ job.data.description }}</span>
    </div>
  {% endfor %}
</div>
