<h1>CV</h1>

{% set cvlist = collections.cv %}
<div class="cvlist">
  {% for entry in cvlist | reverse %}
    <div class="cvlist__item">
      <h2 class="cvlist__title">
        <a class="cvlist__link" href="{{ entry.url | url }}">{{ entry.data.title }}</a>
      </h2>
      <p class="cvlist__description">{{ entry.data.meta }}</p>
    </div>
  {% endfor %}
</div>
